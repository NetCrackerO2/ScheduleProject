<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity"
          prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.101"
          fx:controller="gui.MainForm">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints maxHeight="169.0" minHeight="0.0" prefHeight="28.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="404.0" minHeight="10.0" prefHeight="372.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <GridPane prefHeight="295.0" prefWidth="631.0" GridPane.rowIndex="2">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="289.0" minWidth="10.0" prefWidth="154.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="669.0" minWidth="10.0" prefWidth="646.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <AnchorPane prefHeight="332.0" prefWidth="153.0">
            <Button fx:id="facultyButton" layoutX="30.0" layoutY="29.0" mnemonicParsing="false"
                    onAction="#showFaculty" prefHeight="40.0" prefWidth="87.0" text="Факультеты"/>
            <Button fx:id="cathedraButton" layoutX="30.0" layoutY="144.0" mnemonicParsing="false"
                    onAction="#showCathedra" prefHeight="40.0" prefWidth="87.0" text="Кафедры"/>
            <Button fx:id="groupButton" layoutX="30.0" layoutY="88.0" mnemonicParsing="false"
                    onAction="#showGroup" prefHeight="40.0" prefWidth="87.0" text="Группы"/>
            <Button fx:id="accountButton" layoutX="30.0" layoutY="202.0" mnemonicParsing="false"
                    onAction="#showAccount" prefHeight="40.0" prefWidth="87.0" text="Аккаунты"/>
            <Button fx:id="roleButton" layoutX="30.0" layoutY="257.0" mnemonicParsing="false"
                    onAction="#showRole" prefHeight="40.0" prefWidth="87.0" text="Роли"/>
        </AnchorPane>
        <Pane fx:id="framePane" prefHeight="500.0" prefWidth="700.0" GridPane.columnIndex="1"/>
    </GridPane>
    <AnchorPane prefHeight="20.0" prefWidth="620.0" GridPane.rowIndex="1">
        <MenuBar layoutY="1.0" prefHeight="26.0" prefWidth="620.0" AnchorPane.leftAnchor="0.0"
                 AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="1.0">
            <Menu mnemonicParsing="false" text="File">
                <MenuItem mnemonicParsing="false" text="Close"/>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
                <MenuItem mnemonicParsing="false" text="Delete"/>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
                <MenuItem mnemonicParsing="false" text="About"/>
            </Menu>
        </MenuBar>
    </AnchorPane>
</GridPane>
